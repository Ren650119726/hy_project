<div role="form-group">
    <form class="navbar-form navbar-left" role="search" method='get' action='/appcenter/biz/query.do'>
        <div class="form-group">
            <span>企业标志码：</span>
            <input type="text" name="biz_code" class="form-control">
            <span>管理员手机号：</span>
            <input type="text" name="mobile" class="form-control">
        </div>
        <button type="submit" class="btn btn-default">查询</button>
        #if($!{userDO.level} <3)
        <a class="btn btn-default" href="/appcenter/biz/add.html">新建企业</a>
        #end
    </form>
</div>
<div role="content">
    <table class="table table-striped">
        <thead>
        <tr>
            <th>id</th>
            <th>标志码</th>
            <th>名称</th>
            <th>管理员</th>
            <th>手机号</th>
            <th>邮箱</th>
            <th>描述</th>
            <th>操作</th>
        </tr>
        </thead>
        <tbody>
        #foreach(${bizInfo} in ${bizInfoList})
        <tr>
            <td>$!{bizInfo.id}</td>
            <td>$!{bizInfo.bizCode}</td>
            <td>$!{bizInfo.bizName}</td>
            <td>$!{bizInfo.administrator}</td>
            <td>$!{bizInfo.mobile}</td>
            <td>$!{bizInfo.email}</td>
            <td>$!{bizInfo.bizDesc}</td>
            <td>
                <a href="/appcenter/biz/get.html?biz_code=$!{bizInfo.bizCode}">
                    #if($!{userDO.level} < 3)
                    修改
                    #else
                    查看
                    #end
                </a>
                <a href="/appcenter/biz/delete.do?biz_code=$!{bizInfo.bizCode}">
                    #if($!{userDO.level} < 2)
                    删除
                    #end
                </a>
            </td>
        </tr>
        #end

        </tbody>
    </table>
    <nav class="navbar-right">
        <ul class="pagination">
            <li><a href="#">上一页</a></li>
            <li><a href="#">1</a></li>
            <li><a href="#">2</a></li>
            <li><a href="#">3</a></li>
            <li><a href="#">下一页</a></li>
        </ul>
    </nav>
</div>