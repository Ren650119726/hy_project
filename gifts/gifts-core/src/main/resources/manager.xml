    <bean id="actionGiftManager" class="com.mockuai.giftscenter.core.manager.impl.ActionGiftManagerImpl"/>
    <bean id="marketingManager" class="com.mockuai.giftscenter.core.manager.impl.MarketingManagerImpl"/>
