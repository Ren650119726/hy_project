    <sqlMap resource="sqlmap/sysFieldMap_SqlMap.xml"/>
    <sqlMap resource="sqlmap/sysConfig_SqlMap.xml"/>
    <sqlMap resource="sqlmap/erpOrder_SqlMap.xml"/>
    <sqlMap resource="sqlmap/message_record_sqlmap.xml"/>
    <sqlMap resource="sqlmap/default_express_SqlMap.xml"/>
    <sqlMap resource="sqlmap/duibaRecord_order_SqlMap.xml"/>
    <sqlMap resource="sqlmap/duiba_configuration_SqlMap.xml"/>
    <sqlMap resource="sqlmap/version_deploy_SqlMap.xml"/>
</sqlMapConfig>